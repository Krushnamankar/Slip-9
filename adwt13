db.competitions.insertMany([
 { cid:1, comp:"Programming", sid:1, rank:1 },
 { cid:2, comp:"Programming", sid:3, rank:2 },
 { cid:3, comp:"Programming", sid:6, rank:3 },

 { cid:4, comp:"E-Rangoli", sid:2, rank:1 },
 { cid:5, comp:"E-Rangoli", sid:3, rank:2 },
 { cid:6, comp:"E-Rangoli", sid:8, rank:3 },

 { cid:7, comp:"Drawing", sid:5, rank:1 },
 { cid:8, comp:"Drawing", sid:9, rank:2 },
 { cid:9, comp:"Drawing", sid:10, rank:3 },

 { cid:10, comp:"Singing", sid:4, rank:1 }
])



Queries


1.db.competitions.aggregate([
 { $group: { _id:"$comp", students:{ $sum:1 } } }
])

2.db.competitions.find({ comp:"Programming" }).count()

3.db.competitions.find({ rank:{ $lte:3 } })

4.db.students.find({ class:"FY" })

5.db.competitions.aggregate([
 { $match:{ comp:"E-Rangoli" } },
 { $lookup:{
       from:"students",
       localField:"sid",
       foreignField:"sid",
       as:"stud"
 }},
 { $match:{ "stud.class":"FY" } }
])



